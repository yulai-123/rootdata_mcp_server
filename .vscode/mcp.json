{
    "servers": {
        "time": {
            "command": "python3",
            "args": ["-m", "mcp_server_time", "--local-timezone=Asia/Shanghai"],
            "env": {
                "PATH": "${workspaceFolder}/.venv/bin:$PATH",
            },
          },
          "mcp-rootdata": {
            "command": "python3",
            "args": ["${workspaceFolder}/main.py"],
            "env": {
                "PATH": "${workspaceFolder}/.venv/bin:$PATH",
            },
          }
    }
}